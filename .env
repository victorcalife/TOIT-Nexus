# TOIT NEXUS - Local Development Environment Variables

# Application Configuration
APP_NAME=TOIT NEXUS
SUPPORT_EMAIL=toit@suporte.toit.com.br

# Backend Variables
DATABASE_URL=postgresql://localhost:5432/toitnexus
SESSION_SECRET=toit-nexus-session-secret-local-dev
NODE_ENV=development

# Stripe Variables (use test keys for local development)
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret_here

# Frontend Variables
VITE_API_URL=http://localhost:5000/api

# IBM Quantum Configuration
# Configure your IBM Quantum token here for real quantum hardware access
# Get your token from: https://quantum-computing.ibm.com/
IBM_SECRET=your_ibm_quantum_token_here

# Quantum System Configuration
QUANTUM_MODE=simulation
QUANTUM_CACHE_ENABLED=true
QUANTUM_BATCH_SIZE=100
QUANTUM_TIMEOUT=30000